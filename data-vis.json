{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Crude Birth Rate by Country",
  "data": {
    "url": "https://raw.githubusercontent.com/bdan0005/3179-Week9Homework/main/ne_110m.json",
    "format": {"type": "csv"}
  },
  "transform": [
    {
      "lookup": "Code",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/bdan0005/3179-Week9Homework/main/crude-birth-rate-cleaned.csv",
          "format": {"type": "csv"}
        },
        "key": "properties.NAME",
        "fields": ["BirthRate"]
      }
    }
  ],
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/bdan0005/3179-Week9Homework/main/ne_110m.json",
        "format": {"type": "topojson", "feature": "properties.NAME"}
      },
      "mark": "geoshape",
      "encoding": {
        "color": {
          "field": "BirthRate",
          "type": "quantitative",
          "scale": {"scheme": "viridis"},
          "title": "Crude Birth Rate"
        },
        "tooltip": [
          {"field": "Entity", "title": "Country"},
          {"field": "BirthRate", "title": "Crude Birth Rate"}
        ]
      }
    }
  ]
}
